<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.lollol23.test.lesson06.dao.ReservationDAO">
 	<select id="selectReservation" resultType="com.lollol23.test.lesson06.model.Reservation">
 		SELECT
 			`id`
 			,`name`
 			,`headcount`
 			,`day`
 			,`date`
 			,`phoneNumber`
 			,`state`
 			,`createdAt`
 			,`updatedAt`
 		FROM
 			`booking`
 	</select>
 </mapper>